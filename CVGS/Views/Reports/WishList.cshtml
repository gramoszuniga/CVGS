@model IEnumerable<CVGS.ViewModels.WishList>

@{
    ViewBag.Title = "Wishlist Report for " + ViewBag.userName;
}

<h2>@ViewBag.Title</h2>

<table class="table">
    <tr>
        <th>
            Game Title
        </th>
        <th>
            Physical Price
        </th>
        <th>
            Downloadable Price
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.title)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.phyPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.downPrice)
            </td>
        </tr>
    }
</table>